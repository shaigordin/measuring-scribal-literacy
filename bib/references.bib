@Article{Mashiach.Davidovich2023,
  author     = {Mashiach, Avraham and Davidovich, Uri},
  journal    = {Tel Aviv},
  title      = {The {En}-{Gedi} {Spring} {Site} and the {Judahite} {Expansion} into the {Judaean} {Desert} in the {Late} {Iron} {Age}},
  year       = {2023},
  issn       = {0334-4355},
  month      = jan,
  number     = {1},
  pages      = {21--43},
  volume     = {50},
  abstract   = {This article discusses the results of the excavations conducted in the Iron II site near the En-Gedi Spring in 1961–1962 and 2019. The site, consisting of a prominent stone platform documented as early as the 19th century and other recently discovered structural remains, is interpreted as a Judahite outpost built in a strategic location within the oasis of En-Gedi. On the basis of the ceramic assemblage, it is suggested that this site was founded during the early 7th century BCE and was abandoned before the end of that century—making it the earliest Iron Age occupation in the oasis. Combined with historical considerations and a regional analysis, the En-Gedi Spring site enhances our understanding of the Judahite expansion into the Judaean Desert during the late Iron Age.},
  doi        = {10.1080/03344355.2023.2190273},
  file       = {:Mashiach.Davidovich2023.pdf:PDF},
  groups     = {UnitoProject},
  keywords   = {Judahite monarchy, Iron Age IIB–IIC, En-Gedi, Dead Sea, Judaean Desert, Neo-Assyrian period},
